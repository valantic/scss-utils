  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/styling-template/issues"
  },
  "homepage": "https://github.com/valantic/styling-template",
  "dependencies": {
  },
  "devDependencies": {
    "sass": "~1.85.1",
    "stylelint": "~16.14.1",
    "stylelint-config-valantic": "~9.0.0"
  }
}
